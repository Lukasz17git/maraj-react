{
   "name": "maraj-react",
   "version": "0.0.0",
   "description": "Maraj React Zustand Wrapper and Maraj React Forms.",
   "keywords": [
      "maraj",
      "zustand",
      "forms"
   ],
   "author": "Lukasz Maraj",
   "license": "MIT",
   "private": false,
   "main": "dist/index.js",
   "module": "dist/index.mjs",
   "types": "dist/index.d.ts",
   "files": [
      "dist",
      "README.md",
      "LICENSE"
   ],
   "scripts": {
      "dev": "vitest",
